<div class="contents row" >
  <% @reviews.each do |review| %>
    <div class="content_post" style="background-image: url(<%= review.image %>);">
      <%= simple_format(review.text) %>
      <span class="name">
        <a href="">
          <span>投稿者</span><%= review.user.nickname %>
        </a>
      </span>
    </div>
  <% end %>
  <%= paginate(@reviews) %>
</div>